#[help]
#	format = info
#[info]
#	viewer = cat
#[web]
#	browser = firefox
#[man]
#	viewer = notepad.exe

# You can use git config --global|local help.format "web|info|man"
# Then web.browser "your browser"
# Or man.viewer "your viewer"

#file 'config' location is yourgit_folder/.git/config
